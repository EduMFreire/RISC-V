                Rs1_data_out       &lt;= Rs1_data_in;
                Rs2_data_out       &lt;= Rs2_data_in;
                Imm_out       &lt;= Imm_in;
                Rs1_out         &lt;= Rs1_in;
                Rs2_out         &lt;= Rs2_in;
                Rd_out         &lt;= Rd_in;
                funct_3_out &lt;= funct_3_in;
                funct_7_5 &lt;= funct_7_in(5);
                pc_out &lt;= pc_in;
                alu_pc_out &lt;= alu_pc_in;
                pc4_out &lt;= pc4_in;
                immSrc_out &lt;= immSrc_in;
                size_out &lt;= size_in;
            end if;
        end if;
    end process;
end Behavioral;</string>
